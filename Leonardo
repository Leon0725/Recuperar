public class Consulta {
  String cliente;
  String data;
  String hora;

  public void marcarData(String data){
    this.data = data;
  }
  public void marcaHora(String hora){
    this.hora = hora;
  }
  public void definirCliente(String cliente){
    this.cliente = cliente;
  }
  public String getDados(){
    return " marcada para " + this.data + "  " +  "Consulta do cliente " + this.cliente  + this.hora;
  }
  
}

public class Refeição {
  String cliente;
  String data;
  String hora;
  String nomeDaRefeicao;
  double caloriasGanhas;

  public void marcardata(String data){
    this.data = data;
  }

  public void marcaHora(String hora){
    this.hora = hora;
  }

  public void definirCliente (String cliente){
    this.cliente = cliente;
  }

 public String getDados(){
  return "Refeição do cliente " + this.cliente + "marcada para " + this.data + " " + this.hora;
 }
 
}


import java.util.List;

public class BateriaDeExercicios {
   

  private String cliente;
  private String data;
  private String hora;
  private List<String> exercicios;
  private double caloriasPerdidas;

  public void marcardata(String data){
    this.data = data;
  }
  public void marcaHora(String hora){
    this.hora = hora;
  }
  public void definirCliente(String cliente){
    this.cliente = cliente;
  }
  public String getDados(){
    return " Bateria de exercicios do cliente " + this.cliente + " marcada para " + this.data + " " + this.hora;
  }
}
